{
  "name": "with-local-tmp-dir",
  "version": "2.0.0",
  "main": "src/index.js",
  "repository": "https://github.com/dword-design/with-local-tmp-dir",
  "author": "Sebastian Landwehr <info@dword-design.de>",
  "license": "MIT",
  "description": "Creates a temporary folder inside CWD and passes the path to a function. Afterwards, the folder is removed. Especially useful for testing.",
  "keywords": [
    "test",
    "temp",
    "tmp",
    "temporary",
    "directory",
    "path",
    "cwd",
    "local",
    "remove",
    "cleanup",
    "function",
    "with"
  ],
  "scripts": {
    "prepare:config-files": "dw-config-files",
    "prepare:readme": "mos",
    "prepare": "npm-run-all prepare:*",
    "pretest": "install-self",
    "test:readme": "mos test",
    "test:unit": "nyc --reporter lcov --reporter text mocha",
    "test": "npm-run-all test:*"
  },
  "mos": {
    "installation": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "dependencies": {
    "tmp-promise": "^2.0.2"
  },
  "devDependencies": {
    "@dword-design/config-files": "^1.0.3",
    "expect": "^24.9.0",
    "fs-extra": "^8.1.0",
    "husky": "^3.0.5",
    "install-self": "dword-design/install-self#fork",
    "mocha": "^6.2.2",
    "mos": "dword-design/mos#fork/mos",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "readme-md-generator": "^0.6.2"
  }
}
